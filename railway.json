{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "runtime": "V2",
    "numReplicas": 1,
    "sleepApplication": false,
    "multiRegionConfig": {
      "us-east4-egdc4a": {
        "numReplicas": 1
      }
    }
  },
  "restartPolicyType": "on-failure",
  "restartPolicyMaxRetries": 10
}
